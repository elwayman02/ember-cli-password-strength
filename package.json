{
  "name": "ember-cli-password-strength",
  "version": "0.2.1",
  "description": "Ember-CLI wrapper for password-strength checking via https://github.com/dropbox/zxcvbn",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "deploy": "ember github-pages:commit --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\"; git push; git checkout -",
    "start": "ember server",
    "test": "ember try:each"
  },
  "repository": "https://github.com/elwayman02/ember-cli-password-strength",
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "Jordan Hawker <hawker.jordan@gmail.com> (http://www.JordanHawker.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elwayman02/ember-cli-password-strength/issues"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.2",
    "ember-cli": "2.9.1",
    "ember-cli-app-version": "^2.0.0",
    "ember-cli-content-security-policy": "0.5.0",
    "ember-cli-dependency-checker": "^1.2.0",
    "ember-cli-github-pages": "0.1.2",
    "ember-cli-htmlbars": "^1.0.3",
    "ember-cli-htmlbars-inline-precompile": "^0.3.1",
    "ember-cli-inject-live-reload": "^1.4.0",
    "ember-cli-jshint": "^2.0.0",
    "ember-cli-qunit": "^3.0.2",
    "ember-cli-release": "1.0.0-beta.2",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-test-loader": "^1.1.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-export-application-global": "^1.0.5",
    "ember-load-initializers": "^0.5.1",
    "ember-resolver": "^2.0.3",
    "loader.js": "^4.0.7"
  },
  "keywords": [
    "ember-addon",
    "password",
    "zxcvbn"
  ],
  "dependencies": {
    "ember-cli-babel": "^5.1.6"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "http://jhawk.co/e-c-password-strength-demo"
  }
}
